import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		sc.nextLine();
		for(int i=0;i<a;i++){
		    String b=sc.nextLine();
		    if(b.charAt(0)<b.charAt(1)){
		        System.out.println(( (b.charAt(0)-'a')*26 ) + (b.charAt(1) -'a')  -(b.charAt(0)-'a') );
		    }
		    else{
		        System.out.println( ((b.charAt(0)-'a')*26)+(b.charAt(1)-'a')-(b.charAt(0)-'a')+1);
		    }
		}
	}
}
